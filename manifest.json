{
  "manifest_version": 3,
  "name": "Selected Element Viewer",
  "version": "1.0",
  "description": "Displays the HTML of the selected box.",
  "permissions": ["storage", "scripting", "activeTab"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }
}
